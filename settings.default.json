                        "authors*",
                        "date-range*",
                        "conference*",
                        "location",
                        "doi"
                    ],
                    "template": {
                        "print": "{authors} {title}. {conference}, {location} ({date-range}).",
                        "title": "{title}",
                        "subtitle": "{authors}<br> {conference}, {location} ({date-range})"
                    }
                }
            }
        },
        "lecture": {
            "id": "lecture",
            "icon": "chalkboard-teacher",
            "color": "#ecaf00",
            "name": "Lectures",
            "name_de": "Vortr\u00e4ge",
            "subtypes": {
                "lecture": {
                    "id": "lecture",
                    "icon": "chalkboard-teacher",
                    "name": "Lecture",
                    "name_de": "Vortrag",
                    "modules": [
                        "title*",
                        "authors*",
                        "date-range*",
                        "lecture-type*",
                        "lecture-invited",
                        "conference*",
                        "location",
                        "doi"
                    ],
                    "template": {
                        "print": "{authors} {title}. {conference}, {location}. {date-range}. ({lecture-invited}: {lecture-type})",
                        "title": "{title}",
                        "subtitle": "{authors}<br> {conference}, {location}. {date-range}, {lecture-invited}: {lecture-type}"
                    }
                }
            }
        },
        "review": {
            "id": "review",
            "icon": "article",
            "color": "#1fa138",
            "name": "Reviews & Editorials",
            "name_de": "Reviews & Editorials",
            "subtypes": {
                "review": {
                    "id": "review",
                    "icon": "article",
                    "name": "Peer-Review",
                    "name_de": "Peer-Review",
                    "modules": [
                        "date*",
                        "journal*",
                        "scientist*"
                    ],
                    "template": {
                        "print": "{scientist} Reviewer f\u00fcr <i>{journal}</i>. {month} {year}.",
                        "title": "Reviewer f\u00fcr <i>{journal}</i>.",
                        "subtitle": "{scientist} ({month} {year})"
                    }
                },
                "editorial": {
                    "id": "editorial",
                    "icon": "user-list",
                    "name": "Editorial board",
                    "name_de": "Editorenschaft",
                    "modules": [
                        "date-range-ongoing*",
                        "journal*",
                        "scientist*",
                        "editorial"
                    ],
                    "template": {
                        "print": "{authors} Mitglied des Editorial Board von <i>{journal}</i> ({editorial}). {date-range-ongoing}.",
                        "title": "Mitglied des Editorial Board von <i>{journal}</i> ({editorial})",
                        "subtitle": "{authors} ({date-range-ongoing})"
                    }
                },
                "thesis-rev": {
                    "id": "thesis-rev",
                    "icon": "graduation-cap",
                    "name": "Thesis review",
                    "name_de": "Review einer Abschlussarbeit",
                    "modules": [
                        "date*",
                        "title*",
                        "thesis*",
                        "scientist*"
                    ],
                    "template": {
                        "print": "{authors} Begutachtung einer {thesis}: <i>{title}</i>. {month} {year}.",
                        "title": "Begutachtung einer {thesis}: <i>{title}</i>",
                        "subtitle": "{authors} ({month} {year})"
                    }
                },
                "grant-rev": {
                    "id": "grant-rev",
                    "icon": "clipboard-text",
                    "name": "Other review",
                    "name_de": "Sonstiges Review",
                    "modules": [
                        "date*",
                        "title*",
                        "scientist*",
                        "review-type*"
                    ],
                    "template": {
                        "print": "{authors} {review-type|Begutachtung eines Forschungsantrages}: <i>{title}</i>. {month} {year}.",
                        "title": "{review-type|Begutachtung eines Forschungsantrages}: <i>{title}</i>",
                        "subtitle": "{authors} ({month} {year})"
                    }
                }
            }
        },
        "teaching": {
            "id": "teaching",
            "icon": "chalkboard-simple",
            "color": "#5f272a",
            "name": "Teaching course",
            "name_de": "Lehrveranstaltung",
            "subtypes": {
                "teaching": {
                    "id": "teaching",
                    "icon": "chalkboard-simple",
                    "name": "Teaching",
                    "name_de": "Lehre",
                    "modules": [
                        "teaching-course*",
                        "teaching-category*",
                        "supervisor*",
                        "semester-select",
                        "date-range*"
                    ],
                    "template": {
                        "print": "{supervisor} {teaching-category} - <i>{teaching-course}</i>. {date-range}.",
                        "title": "{teaching-category} - {teaching-course}",
                        "subtitle": "{supervisor} ({date-range}) "
                    }
                },
                "theses": {
                    "id": "theses",
                    "icon": "graduation-cap",
                    "name": "Theses",
                    "name_de": "Abschlussarbeiten",
                    "modules": [
                        "title*",
                        "authors*",
                        "person*",
                        "student-category",
                        "details",
                        "status*",
                        "date-range*"
                    ],
                    "template": {
                        "print": "{person}, {affiliation}. {title}; {category} ({details}), {date-range} ({status}), betreut von {authors}",
                        "title": "{person}, {affiliation}. <i>{title}</i>",
                        "subtitle": "betreut von {authors}<br> {category} ({details}), {date-range} ({status})"
                    }
                }
            }
        },
        "software": {
            "id": "software",
            "icon": "desktop-tower",
            "color": "#9a499c",
            "name": "Software & Data",
            "name_de": "Software & Daten",
            "subtypes": {
                "software": {
                    "id": "software",
                    "icon": "desktop-tower",
                    "name": "Software & Data",
                    "name_de": "Software & Daten",
                    "modules": [
                        "title*",
                        "authors*",
                        "date*",
                        "software-venue",
                        "software-link",
