    "activities": {
        "publication": {
            "id": "publication",
            "icon": "book-bookmark",
            "color": "#006eb7",
            "name": "Publications",
            "name_de": "Publikationen",
            "subtypes": {
                "article": {
                    "id": "article",
                    "icon": "file-text",
                    "name": "Journal Article",
                    "name_de": "Journal Article",
                    "modules": [
                        "title*",
                        "authors*",
                        "date*",
                        "journal*",
                        "issue",
                        "volume",
                        "pages",
                        "doi",
                        "pubmed",
                        "openaccess",
                        "online-ahead-of-print",
                        "correction"
                    ],
                    "template": {
                        "print": "{authors} ({year}) {title}. <i>{journal-abbr}</i> {volume-issue-pages}. {doi} {online-ahead-of-print} {openaccess}",
                        "title": "{title}",
                        "subtitle": "{authors}<br> <i>{journal-abbr}</i> {volume-issue-pages} ({year}) {online-ahead-of-print} {openaccess}"
                    }
                },
                "magazine": {
                    "id": "magazine",
                    "icon": "newspaper",
                    "name": "Non-Refereed",
                    "name_de": "Non-refereed",
                    "modules": [
                        "title*",
                        "authors*",
                        "date*",
                        "magazine*",
                        "link",
                        "doi",
                        "pubmed"
                    ],
                    "template": {
                        "print": "{authors} ({year}) {title}. <i>{magazine}</i> {link}",
                        "title": "{title}",
                        "subtitle": "{authors}<br> <i>{magazine}</i> ({year})"
                    }
                },
                "book": {
                    "id": "book",
                    "icon": "book",
                    "name": "Book",
                    "name_de": "Buch",
                    "modules": [
                        "title*",
                        "authors*",
                        "date*",
                        "book-series",
                        "edition",
                        "volume",
                        "pages",
                        "publisher",
                        "city",
                        "doi",
                        "pubmed",
                        "isbn",
                        "openaccess"
                    ],
                    "template": {
                        "print": "{authors} ({year}) <i>{title}</i> {book-series}. {publisher}. {doi}",
                        "title": "{title}",
                        "subtitle": "{authors}<br> {book-series}. {publisher}. ({year})"
                    }
                },
                "chapter": {
                    "id": "chapter",
                    "icon": "book-bookmark",
                    "name": "Book chapter",
                    "name_de": "Buchkapitel",
                    "modules": [
                        "title*",
                        "authors*",
                        "date*",
                        "edition",
                        "volume",
                        "pages",
                        "book-title*",
                        "publisher",
                        "city",
                        "editor",
                        "doi",
                        "pubmed",
                        "isbn",
                        "openaccess"
                    ],
                    "template": {
                        "print": "{authors} ({year}) {title}. In: {editor} (eds). <i>{book-title}</i> ({edition}th ed., pp. {pages}) {city}: {publisher}. {doi}",
                        "title": "{title}. In: <i>{book-title}</i> ",
                        "subtitle": "{authors}<br> {editor} (eds). ({edition} ed., pp. {pages}) {city}: {publisher} ({year})"
                    }
                },
                "preprint": {
                    "id": "preprint",
                    "icon": "paper-plane-tilt",
                    "name": "Preprint",
                    "name_de": "Preprint",
                    "modules": [
                        "title*",
                        "authors*",
                        "date*",
                        "doi",
                        "pubmed",
                        "magazine*",
                        "link"
                    ],
                    "template": {
                        "print": "{authors} ({year}) {title} <i>{magazine}</i> [Preprint] {doi}",
                        "title": "{title} [Preprint]",
                        "subtitle": "{authors}<br> <i>{magazine}</i> ({year})"
                    }
                },
                "dissertation": {
                    "id": "dissertation",
                    "icon": "graduation-cap",
                    "name": "Thesis",
                    "name_de": "Abschlussarbeit",
                    "modules": [
                        "title*",
                        "authors*",
                        "date*",
                        "university*",
                        "city*",
                        "thesis*",
                        "doi",
                        "pubmed",
                        "isbn",
                        "openaccess"
                    ],
                    "template": {
                        "print": "{authors} ({year}) {title} ({thesis}) {university}, {city}. {doi}",
                        "title": "{title} [{thesis}]",
                        "subtitle": "{authors}, {university}, {city} ({year})"
                    }
                },
                "others": {
                    "id": "others",
                    "icon": "notebook",
                    "name": "Other publication",
                    "name_de": "Weitere Publikationen",
                    "modules": [
                        "title*",
                        "authors*",
                        "date*",
                        "doi",
                        "pubmed",
                        "doctype*"
                    ],
                    "template": {
                        "print": "{authors} ({year}) {title} [{doctype}] {doi}",
                        "title": "{title} [{doctype}]",
                        "subtitle": "{authors} ({year})"
                    }
                }
            }
        },
        "poster": {
            "id": "poster",
            "icon": "presentation-chart",
            "color": "#b61f29",
            "name": "Poster",
            "name_de": "Poster",
            "subtypes": {
                "poster": {
                    "id": "poster",
                    "icon": "presentation-chart",
                    "name": "Poster",
